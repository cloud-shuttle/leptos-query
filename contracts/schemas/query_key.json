{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://api.leptos-query.rs/schemas/query_key.json",
  "title": "QueryKey",
  "description": "A unique identifier for a query, represented as an object with segments array",
  "type": "object",
  "properties": {
    "segments": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255
      },
      "minItems": 1,
      "maxItems": 10,
      "additionalItems": false
    }
  },
  "required": ["segments"],
  "additionalProperties": false,
  "examples": [
    {
      "segments": ["user", "123"]
    },
    {
      "segments": ["users"]
    },
    {
      "segments": ["posts", "user", "123"]
    },
    {
      "segments": ["settings", "theme"]
    },
    {
      "segments": ["api", "v1", "users", "123", "posts"]
    }
  ]
}
